{
  "type": "setting",
  "settings": 
  {
    "editor.codeActionsOnSave": 
    {
      "source.fixAll.eslint": true
    },
    "eslint.nodePath": "./node_modules",
    "eslint.workingDirectories": [
      {
        "mode": "auto"
      }
    ],
    "eslint.packageManager": "npm",
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
    ]
  }
}